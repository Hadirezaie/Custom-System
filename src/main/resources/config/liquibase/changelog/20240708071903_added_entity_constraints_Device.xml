<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Device.
    -->
    <changeSet id="20240708071903-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="trader_id"
                                 baseTableName="device"
                                 constraintName="fk_device__trader_id"
                                 referencedColumnNames="id"
                                 referencedTableName="trader"/>

        <addForeignKeyConstraint baseColumnNames="tarif_id"
                                 baseTableName="device"
                                 constraintName="fk_device__tarif_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tarif"/>
    </changeSet>
</databaseChangeLog>
